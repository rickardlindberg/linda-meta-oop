Examples {
    run   = %:x !. -> x

    Start =        -> put(["Greet" "World"])
                   -> put(["Greet" "Rickard"])
                   -> put(["Fact" 6])

    Fact  =
        | 0   .:y .:z -> print({"Factorial of " y " is " mul(z)})
        | .:x .:y .:z -> put(["Fact" sub([x 1]) y append(z x)])
        | .:x         -> put(["Fact" x          x []])

    Greet = .:name -> print({"Hello " name "!"})
}
